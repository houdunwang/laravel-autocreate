{
    "name": "houdunwang/laravel-autocreate",
    "description": "auto create laravel controller view",
    "license": "MIT",
    "authors": [
        {
            "name": "houdunwang",
            "email": "2300071698@qq.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "\\Houdunwang\\AutoCreate\\LaravelServiceProvider"
            ],
            "aliases": {
                "HDModule": "\\Houdunwang\\Module\\Factory"
            }
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3",
        "doctrine/dbal": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Houdunwang\\AutoCreate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "houdunwang\\AutoCreate\\Tests\\": "tests/"
        }
    }
}
